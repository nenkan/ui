{
  "name": "@nenkan/ui",
  "version": "0.22.0",
  "description": "Reusable React components following Nenkan styles",
  "exports": "./dist/index.js",
  "private": true,
  "scripts": {
    "build": "node ./scripts/clean.js && babel src --out-dir dist --copy-files --source-maps --extensions \".ts,.tsx\" && node ./scripts/build.js",
    "build:watch": "nodemon --watch src --exec npm run build",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "release": "npm run build && cd dist && npm publish"
  },
  "type": "module",
  "author": "nenkan",
  "homepage": "https://www.nen-kan.com/ui",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.5.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-markdown": "^9.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nenkan/ui.git"
  },
  "source": true,
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@nenkan/css": "^0.21.0",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.14.0",
    "eslint-plugin-react": "^7.37.2",
    "nodemon": "^3.1.7",
    "react-dom": "^18.3.1",
    "sass": "^1.80.7",
    "sass-loader": "^16.0.3",
    "style-loader": "^4.0.0"
  }
}
