{
  "name": "@nenkan/ui",
  "version": "0.18.0",
  "description": "Reusable React components following Nenkan styles",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "node ./scripts/clean.js && babel src --out-dir dist --copy-files --source-maps && node ./scripts/build.js",
    "build:watch": "nodemon --watch src --exec npm run build",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "release": "npm run build && cd dist && npm publish"
  },
  "author": "nenkan",
  "homepage": "https://www.nen-kan.com/ui",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.3.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-markdown": "^6.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nenkan/ui.git"
  },
  "source": true,
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.18.6",
    "@chromatic-com/storybook": "^1.3.5",
    "@nenkan/css": "^0.20.2",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.40.0",
    "eslint-plugin-react": "^7.32.2",
    "nodemon": "^2.0.22",
    "react-dom": "^18.2.0",
    "sass": "^1.63.3",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2"
  }
}
