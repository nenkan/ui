{
  "name": "@nenkan/ui",
  "version": "0.25.0",
  "description": "Reusable React components following Nenkan styles",
  "exports": "./dist/index.js",
  "private": true,
  "scripts": {
    "build": "node ./scripts/clean.js && babel src --out-dir dist --copy-files --source-maps --extensions \".ts,.tsx\" && node ./scripts/build.js",
    "build:watch": "nodemon --watch src --exec npm run build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "release": "npm run build && cd dist && npm publish"
  },
  "type": "module",
  "author": "nenkan",
  "homepage": "https://www.nen-kan.com/ui",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.5.1",
    "react": "^19.2.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nenkan/ui.git"
  },
  "source": true,
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@nenkan/css": "^0.22.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "nodemon": "^3.1.11",
    "react-dom": "^19.2.1",
    "sass": "^1.95.1",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  }
}
