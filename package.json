{
  "name": "@nenkan/ui",
  "version": "0.24.0",
  "description": "Reusable React components following Nenkan styles",
  "exports": "./dist/index.js",
  "private": true,
  "scripts": {
    "build": "node ./scripts/clean.js && babel src --out-dir dist --copy-files --source-maps --extensions \".ts,.tsx\" && node ./scripts/build.js",
    "build:watch": "nodemon --watch src --exec npm run build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "release": "npm run build && cd dist && npm publish"
  },
  "type": "module",
  "author": "nenkan",
  "homepage": "https://www.nen-kan.com/ui",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.5.1",
    "react": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-markdown": "^10.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nenkan/ui.git"
  },
  "source": true,
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.36.0",
    "@nenkan/css": "^0.21.1",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^16.4.0",
    "nodemon": "^3.1.7",
    "react-dom": "^18.3.1",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.45.0"
  }
}
